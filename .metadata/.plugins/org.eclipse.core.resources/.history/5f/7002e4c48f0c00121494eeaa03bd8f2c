import MySQLdb

class myDb(object):
    '''
    Clase mi base de datos. Se encarga de interaccionar con la base de datos MySql
    '''
   
    

    def __init__(self,host,user,userpwd,database):
        '''
        Constructor
        '''
         __host = host
         __user = user
         __userpwd = userpwd
         __database = database